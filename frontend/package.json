    "prepare": "cd .. && husky install frontend/.husky"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@react-mock/fetch": "^0.3.0",
    "@testing-library/dom": ">=5",
    "@testing-library/jest-dom": "^4.2.4",
